                            & > a {
                                display: inline;
                            }
                        }
                    }
                }                
            }
        }
    }
</style>
<script>
    let allItems = document.querySelectorAll('.contact-main-item');
    allItems.forEach((el, idx) => {
        el.querySelector('.head-wrap').addEventListener('click', function(e) {
            e.preventDefault();
            if (el.querySelector('.head-wrap').classList.contains('active')) {
                el.querySelector('.head-wrap').classList.remove('active');
                el.querySelector('.body-wrap').removeAttribute('style');
                
            } else {
                el.querySelector('.head-wrap').classList.add('active');
                let height = el.querySelector('.body-wrap-inner').getBoundingClientRect().height;
                el.querySelector('.body-wrap').setAttribute('style', 'max-height: ' + height + 'px')
            }

        })
    })
</script>
