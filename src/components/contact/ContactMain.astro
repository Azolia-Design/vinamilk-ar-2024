                column-gap: 1rem;
                color: var(--cl-bg);
                background-color: var(--cl-blue);
                transition: background-color 400ms ease;
                padding-block: .8rem;
                padding-inline: 2rem;
                border-radius: 1.4rem;
                .ic {
                    transition: transform 400ms ease;
                }
                &:hover {
                    background-color: var(--cl-blue-2);
                }
                &.active {
                    .ic {
                        transform: rotate(180deg);
                    }
                }
            }
            .body-wrap {
                max-height: 0;
                overflow: hidden;
                transition: max-height 600ms ease;
                &-inner {
                    padding-top: 3.8rem;
                    display: grid;
                    grid-template-columns: repeat(3, 1fr);
                    column-gap: var(--grid-gap);
                    row-gap: 4.4rem;
                    .ctc-main-sub-item {
                        display: flex;
                        flex-direction: column;
                        row-gap: 1.8rem;
                        max-width: 34.2rem;
                        .ctc-main-body-sub-txt {
                            color: var(--cl-blue);
                        }
                        .ctc-main-sub-item-inner-txt {
                            color: var(--cl-txt);
                            .txt-label {
                                color: var(--cl-blue);
                                font-weight: 700;
