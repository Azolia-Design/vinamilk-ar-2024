                        &:nth-child(2) {
                            @media screen and (max-width: 767px) {
                                grid-column: span 2;
                            }
                        }
                        &:nth-child(3) {
                            @media screen and (max-width: 767px) {
                                grid-column: span 2;
                            }
                        }
                    }
                }
            }
            .box-3 {
                background-color: var(--cl-green);
                color: var(--cl-bg);
                margin-bottom: 3.2rem;
                @media screen and (max-width: 767px) {
