                    @media screen and (max-width: 767px) {
                        padding-block: 0 1.3rem;
                        padding-inline: 2rem 4rem;
                    }
                    .number-value {
                        margin-bottom: -1.7rem;
                        @media screen and (max-width: 767px) {
                            margin-bottom: -1.5rem;
                            font-size: 12.8rem;
                            margin-right: 5.3rem;

                        }
                    }
